<table>
  <thead>
    <tr>
      <th>feature</th>
      <th>scenario</th>
      <th>duration</th>
    </tr>
  </thead>
  <tbody>
    <% analysis_results.each do |info| %>
    <tr>
      <td><span><%= info.feature_name %></span></td>
      <td><a href="#<%= info.id %>"><%= info.name %></a></td>
      <td><span><%= info.run_time %></span> sec</td>
    </tr>
    <% end %>
  </tbody>
</table>
